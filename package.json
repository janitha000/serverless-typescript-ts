{
  "name": "serverless-step",
  "version": "1.0.0",
  "description": "Serverless aws-nodejs-typescript template",
  "main": "serverless.ts",
  "scripts": {
    "test": "nyc mocha",
    "test-jest": "jest --config=jest.config.js",
    "deploy": "serverless deploy --verbose",
    "deploy:local": "serverless deploy --stage local --verbose",
    "deploy:ci": "serverless deploy --verbose --stage ",
    "start": "serverless offline start",
    "deploy:func": "serverless deploy function --stage dev --verbose --function",
    "logs": "serverless logs --function",
    "compile": "tsc",
    "debug": "node --inspect ./node_modules/.bin/serverless offline -s dev"
  },
  "engines": {
    "node": ">=14.15.0"
  },
  "dependencies": {
    "@middy/core": "^2.5.3",
    "@middy/http-json-body-parser": "^2.5.3",
    "aws-sdk": "^2.1122.0",
    "chai": "^4.3.6",
    "data-api-client": "^1.3.0",
    "lambda-tester": "^4.0.1",
    "mocha": "^10.0.0",
    "proxyquire": "^2.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@babel/preset-env": "^7.17.10",
    "@babel/preset-typescript": "^7.16.7",
    "@serverless/typescript": "^3.0.0",
    "@types/aws-lambda": "^8.10.71",
    "@types/chai": "^4.3.1",
    "@types/data-api-client": "^1.2.4",
    "@types/jest": "^27.5.0",
    "@types/lambda-tester": "^3.6.1",
    "@types/mocha": "^9.1.1",
    "@types/node": "^14.14.25",
    "babel-jest": "^28.1.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "database": "file:databaseLayer/database/nodejs/node_modules/database",
    "esbuild": "^0.14.11",
    "jest": "^28.1.0",
    "json-schema-to-ts": "^1.5.0",
    "serverless": "^3.0.0",
    "serverless-auto-swagger": "^2.4.1",
    "serverless-aws-documentation": "^1.1.0",
    "serverless-esbuild": "^1.23.3",
    "serverless-localstack": "^0.4.35",
    "serverless-offline": "^8.5.0",
    "serverless-plugin-lambda-insights": "^1.5.0",
    "serverless-step-functions": "^3.7.0",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.3"
  },
  "author": "The serverless webpack authors (https://github.com/elastic-coders/serverless-webpack)",
  "license": "MIT"
}
