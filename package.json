{
  "name": "serverless-step",
  "version": "1.0.0",
  "description": "Serverless aws-nodejs-typescript template",
  "main": "serverless.ts",
  "scripts": {
    "test": "nyc mocha",
    "test-jest": "jest --config=jest.config.js",
    "deploy": "serverless deploy --verbose",
    "deploy:staging": "serverless deploy --stage staging --verbose",
    "deploy:local": "serverless deploy --stage local --verbose",
    "deploy:ci": "serverless deploy --verbose --stage ",
    "start": "serverless offline start",
    "deploy:func": "serverless deploy function --stage dev --verbose --function",
    "logs": "serverless logs --function",
    "compile": "tsc",
    "debug": "node --inspect ./node_modules/.bin/serverless offline -s dev",
    "db:init": "sequelize migration:create --name",
    "db:migrate": "sequelize db:migrate",
    "db:makemigrations": "./node_modules/sequelize-auto-migrations/bin/makemigration.js",
    "doc": "serverless downloadDocumentation  --outputFileName=swagger.yaml --verbose",
    "lint": "eslint ."
  },
  "engines": {
    "node": ">=14.15.0"
  },
  "dependencies": {
    "@middy/core": "^2.5.3",
    "@middy/http-error-handler": "^3.0.3",
    "@middy/http-json-body-parser": "^2.5.3",
    "@middy/http-security-headers": "^3.0.3",
    "@middy/input-output-logger": "^3.0.3",
    "@middy/validator": "^3.0.3",
    "@sentry/serverless": "^6.19.7",
    "aws-sdk": "^2.1055.0",
    "chai": "^4.3.6",
    "data-api-client": "^1.3.0",
    "lambda-tester": "^4.0.1",
    "mocha": "^10.0.0",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "proxyquire": "^2.1.3",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.19.0",
    "sequelize-auto-migrations": "^1.0.3",
    "sequelize-cli": "^6.4.1",
    "sequelize-typescript": "^2.1.3",
    "serverless-plugin-canary-deployments": "^0.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@babel/preset-env": "^7.17.10",
    "@babel/preset-typescript": "^7.16.7",
    "@serverless/typescript": "^3.0.0",
    "@types/aws-lambda": "^8.10.71",
    "@types/chai": "^4.3.1",
    "@types/data-api-client": "^1.2.4",
    "@types/jest": "^27.5.0",
    "@types/lambda-tester": "^3.6.1",
    "@types/mocha": "^9.1.1",
    "@types/node": "^14.18.18",
    "@types/validator": "^13.7.2",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "babel-jest": "^28.1.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "database": "file:databaseLayer/database/nodejs/database",
    "esbuild": "^0.14.11",
    "eslint": "^8.16.0",
    "jest": "^28.1.0",
    "json-schema-to-ts": "^1.5.0",
    "serverless": "^3.0.0",
    "serverless-auto-swagger": "^2.5.0",
    "serverless-aws-documentation": "^1.1.0",
    "serverless-domain-manager": "^6.0.3",
    "serverless-esbuild": "^1.23.3",
    "serverless-layers": "^2.5.4",
    "serverless-localstack": "^0.4.35",
    "serverless-offline": "^8.5.0",
    "serverless-plugin-aws-alerts": "git://github.com/ACloudGuru/serverless-plugin-aws-alerts.git#6a98d42ab2b1916fb0bdeb31717819575d4e6754",
    "serverless-plugin-lambda-insights": "^1.5.0",
    "serverless-step-functions": "^3.7.0",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.3"
  },
  "author": "The serverless webpack authors (https://github.com/elastic-coders/serverless-webpack)",
  "license": "MIT"
}
